---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<author>
		<name>Sebastian Teumert</name>
	</author>
	<title>Sebastian Teumert</title>
	<link href="http://teumert.net/atom.xml" rel="self" />
	<link href="http://teumert.net/" />
	<id>http://teumert.net/</id>
  	<updated>{{ site.time | date_to_xmlschema }}</updated>
 
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://teumert.net/{{ post.url }}" />
		<id>http://teumert.net/{{ post.url }}</id>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<summary type="html"><![CDATA[ {{ post.excerpt }} ]]></summary>
		<content type="html">
			<![CDATA[ {{ post.content }} ]]>
		</content>
	</entry>
	{% endfor %}
</feed>
